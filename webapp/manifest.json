{
	"sap.app": {
	  "id": "myapp",
	  "type": "application",
	  "i18n": "i18n/i18n.properties",
	  "applicationVersion": {
		"version": "1.0.0"
	  },
	  "title": "{{appTitle}}",
	  "description": "{{appDescription}}"
	},
	"sap.ui5": {
	  "async": true,
	  "rootView": {
		"viewName": "myapp.view.MainView",
		"type": "XML",
		"async": true
	  },
	  "models": {
		"data": {
		  "type": "sap.ui.model.json.JSONModel",
		  "uri": "model/data.json"
		},
		"i18n": {
		  "type": "sap.ui.model.resource.ResourceModel",
		  "settings": {
			"bundleName": "myapp.i18n.i18n"
		  }
		}
	  },
	  
	  "resources": {
		"configuration": {
		  "paths": {
			"myapp": "./"
		  }
		}
	  }
	}
  }
  